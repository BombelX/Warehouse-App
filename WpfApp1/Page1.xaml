<Page x:Class="WpfApp1.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1"
      mc:Ignorable="d" 
      d:DesignHeight="1280
      " d:DesignWidth="720
      "
      Title="Page1">

    <Grid x:Name="ds" Background="#FF7BA7A2" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="23*"/>
            <ColumnDefinition Width="217*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Powrot" HorizontalAlignment="Left" Margin="207,10,0,0" VerticalAlignment="Top" Height="46" Width="168" Background="#FFA24242" Grid.Column="1" Click="Button_Click" />
        <ScrollViewer Margin="0,93,0,10" Grid.ColumnSpan="2" x:Name="scrollview">
            <StackPanel x:Name="stack"></StackPanel>
        </ScrollViewer>
        <TextBox x:Name="Barcode" Grid.Column="1" HorizontalAlignment="Left" KeyDown="TextBox_KeyDown" Margin="464,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top"  IsTabStop="False" Width="0" Height="0" />
    </Grid>
</Page>
